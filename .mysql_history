describe provincias;
select pcia_nombre, pcia_codigoib from provincias limit 10;
select pcia_nombre, pcia_codigoib from provincias limit 10, 10;
select pcia_nombre, pcia_codigoib from provincias where pcia_codigoib > 920;
select pcia_nombre, pcia_codigoib from provincias where pcia_codigoib > 921;
