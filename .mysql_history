create database prueba;
use prueba;
CREATE TABLE provincias (id_provincia int(3) unsigned NOT NULL DEFAULT '0', pcia_nombre varchar(20) NOT NULL DEFAULT '', pcia_abreviatura char(3) NOT NULL DEFAULT '', pcia_codigoib int(3) unsigned NOT NULL DEFAULT '0', pcia_letra char(1) NOT NULL DEFAULT '') ENGINE=InnoDB DEFAULT CHARSET=latin1 COMMENT='Tabla de las provincias ';  
INSERT INTO provincias (id_provincia, pcia_nombre, pcia_abreviatura, pcia_codigoib, pcia_letra) VALUES (901, 'CAPITAL FEDERAL', 'CAP', 901, 'C'),(902, 'BUENOS AIRES', 'BAI', 902, 'B'),(903, 'CATAMARCA', 'CAT', 903, 'K'),(904, 'CORDOBA', 'CBA', 904, 'X'),(905, 'CORRIENTES', 'COR', 905, 'W'),(906,'CHACO', 'CHA', 906, 'H'), (907, 'CHUBUT', 'CHU', 907, 'U'), (908, 'ENTRE RIOS', 'ERI', 908, 'E'), (909, 'FORMOSA', 'FOR', 909, 'P'), (910, 'JUJUY', 'JUJ', 910, 'Y'), (911, 'LA PAMPA', 'LAP', 911, 'L'), (912, 'LA RIOJA', 'LAR', 912, 'F'), (913, 'MENDOZA', 'MZA', 913, 'M'), (914, 'MISIONES', 'MIS', 914, 'N'), (915, 'NEUQUEN', 'NEU', 915, 'Q'), (916, 'RIO NEGRO', 'RNE', 916, 'R'), (917, 'SALTA', 'SAL', 917, 'A'), (918, 'SAN JUAN', 'SJU', 918, 'J'), (919, 'SAN LUIS', 'SLU', 919, 'D'), (920, 'SANTA CRUZ', 'SCR', 920, 'Z'), (921, 'SANTA FE', 'SFE', 921, 'S'), (922, 'SANTIAGO DEL ESTERO', 'STG', 922, 'G'), (923, 'TIERRA DEL FUEGO', 'TIE', 923, 'V'), (924, 'TUCUMAN', 'TUC', 924, 'T');
quit;
